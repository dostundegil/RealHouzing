@model List<AboutUsViewModel>
<div class="about-sheltek-area ptb-115">
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
            <div class="col-md-6 col-12">
                <div class="section-title mb-30">
                   
                    <h3>@item.AboutUsTitle</h3>
                </div>
                <div class="about-sheltek-info">
                    <p>
                            @item.AboutUsContent
                    </p>
                </div>
            </div>
            }
            <div class="col-md-6 col-12">
                <div class="about-image">
                    <a href="about.html"><img src="~/sheltek/images/about/1.jpg" alt=""></a>
                </div>
            </div>
        </div>
    </div>
</div>